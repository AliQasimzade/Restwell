{
  "expo": {
    "name": "RestWell",
    "slug": "RestWell",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/images/iconReswell.png",
    "splash": {
      "image": "./app/assets/images/RestWell.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.huynh.listar"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package":"com.restwell.android"
    },

    "web": {
      "favicon": "./app/assets/images/iconRestwell.png"
    }
  }
}
